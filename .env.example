# Root Environment Variables Example
# Copy this file to .env and fill in your actual values
# NEVER commit .env or .env.local to git

# Cron Job Configuration
CRON_SECRET="your-secure-random-token-here"

# Supabase
VITE_PUBLIC_SUPABASE_URL="https://your-project.supabase.co"
SUPABASE_ANON_KEY="your-anon-key-here"
SUPABASE_SERVICE_ROLE_KEY="your-service-role-key-here"

# Database URLs (from Supabase)
POSTGRES_URL="postgres://user:password@host:port/database"
POSTGRES_PRISMA_URL="postgres://user:password@pooler-host:6543/database"
POSTGRES_URL_NON_POOLING="postgres://user:password@host:5432/database"

# Vercel (if deploying to Vercel)
VERCEL_OIDC_TOKEN="your-vercel-oidc-token"
